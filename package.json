{
  "name": "dojocode-embed",
  "version": "1.0.0",
  "description": "A nice greeter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib/**/*"],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danut99/dojocode-embed.git"
  },
  "keywords": ["Dojocode", "oembed"],
  "author": "Danut",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danut99/dojocode-embed/issues"
  },
  "homepage": "https://github.com/danut99/dojocode-embed#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/npm": "^7.19.0",
    "axios": "^1.3.2",
    "dotenv": "^16.0.3"
  }
}
